{
  "hosting": {
    "public": "functions/dist/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "*.[0-9a-f]+.@(css|js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "/@(ngsw-worker.js|ngsw.json)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/@(resolver|standard)/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=300,s-maxage=600"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/@(resolver|standard)/**",
        "function": "ssr"
      },
      {
        "source": "**",
        "destination": "/index.original.html"
      }
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  },
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  }
}
